    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "rimraf": "2.6.3",
    "ts-loader": "6.0.0",
    "ts-node": "8.1.0",
    "typescript": "3.4.5",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.3.1"
  },
  "dependencies": {
    "react": "16.8.6",
