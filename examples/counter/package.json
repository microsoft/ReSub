    "http-server": "14.1.1",
    "rimraf": "3.0.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.4.1",
    "typescript": "3.6.4",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.9.0"
  },
